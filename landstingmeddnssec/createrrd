rrdtool create dnssec.rrd --start -365days --step 86400 DS:dnssec:GAUGE:345600:0:300 RRA:LAST:0.1:1:1000
for m in `seq 6 12` ; do for d in `seq 1 30` ; do rrdtool update dnssec.rrd 2009`printf "%02d%02d" $m $d `@1; done; done
for m in `seq 1 3` ; do for d in `seq 1 30` ; do rrdtool update dnssec.rrd 2010`printf "%02d%02d" $m $d `@1; done; done
for m in `seq 4 5` ; do for d in `seq 1 30` ; do rrdtool update dnssec.rrd 2010`printf "%02d%02d" $m $d `@1; done; done
for m in `seq 6 7` ; do for d in `seq 1 30` ; do rrdtool update dnssec.rrd 2010`printf "%02d%02d" $m $d `@1; done; done

for i in `seq 1 13` ; do
	rrdtool update dnssec.rrd 201008`printf "%02d" $i`@1
done
