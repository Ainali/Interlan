
for i in `seq  1 9 ` ; do
	ERROR=`cat history/20120${i}01/*|grep ERROR|wc -l`
	WARNING=`cat history/20120${i}01/*|grep WARNING|wc -l`
	echo 0${i},$ERROR,$WARNING
done
for i in `seq  10 12 ` ; do
	ERROR=`cat history/2012${i}01/*|grep ERROR|wc -l`
        WARNING=`cat history/2012${i}01/*|grep WARNING|wc -l`
        echo ${i},$ERROR,$WARNING
done
