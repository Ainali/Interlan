rrdtool create dns6.rrd --start -200days --step 86400 DS:dns6:GAUGE:345600:0:300 RRA:LAST:0.1:1:1000
for m in `seq 1 3` ; do for d in `seq 1 30` ; do rrdtool update dns6.rrd 2010`printf "%02d%02d" $m $d `@7; done; done
for m in `seq 4 5` ; do for d in `seq 1 30` ; do rrdtool update dns6.rrd 2010`printf "%02d%02d" $m $d `@11; done; done
for m in `seq 6 7` ; do for d in `seq 1 30` ; do rrdtool update dns6.rrd 2010`printf "%02d%02d" $m $d `@14; done; done

for i in `seq 1 9` ; do
	rrdtool update dns6.rrd 201008`printf "%02d" $i`@14
done
